@model GuildCarsMastery.Models.Reports.ReportInventoryVM
@{
    ViewBag.Title = "Inventory";
}

<h2>Inventory Report</h2>

<h4>New Vehicles</h4>
<table id="new-vehicles">
    <tr>
        <th>Year</th>
        <th>Make</th>
        <th>Model</th>
        <th>Count</th>
        <th>Stock Value</th>
    </tr>
</table>
<br />
<h4>Used Vehicles</h4>
<table id="used-vehicles">
    <tr>
        <th>Year</th>
        <th>Make</th>
        <th>Model</th>
        <th>Count</th>
        <th>Stock Value</th>
    </tr>
</table>